{
    "version": 1,
    "formatters": {
        "default": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "formatter": "default"
        },
        "file": {
            "class": "logging.FileHandler",
            "filename": "pinnacle.log",
            "formatter": "default"
        }
    },
    "loggers": {
        "pinnacle": {
            "level": "DEBUG",
            "handlers": ["console", "file"]
        }
    }
}
